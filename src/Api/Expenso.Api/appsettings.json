{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Application": {
    "Name": "Expenso",
    "Version": "0.0.0"
  },
  "Authentication": {
    "AuthServer": "Keycloak"
  },
  "Keycloak": {
    "realm": "expenso",
    "auth-server-url": "http://localhost:9000/",
    "ssl-required": "none",
    "resource": "expenso_app",
    "verify-token-audience": true,
    "credentials": {
      "secret": ""
    }
  },
  "EfCore": {
    "ConnectionParameters": {
      "Host": "127.0.0.1",
      "Port": "5432",
      "DefaultDatabase": "postgres",
      "Database": "expenso",
      "User": "",
      "Password": ""
    },
    "InMemory": false,
    "UseMigration": true,
    "UseSeeding": true
  },
  "Files": {
    "StorageType": "Disk",
    "ImportDirectory": "Import",
    "ExportDirectory": "Export"
  },
  "Notifications": {
    "InApp": {
      "Enabled": true
    },
    "Email": {
      "Enabled": true,
      "ReplyTo": "",
      "From": "",
      "Smtp": {
        "Host": "127.0.0.1",
        "Port": 25,
        "Ssl": false,
        "Username": "",
        "Password": ""
      }
    },
    "Push": {
      "Enabled": true
    }
  }
}