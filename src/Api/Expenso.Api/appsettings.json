{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ]
  },
  "AllowedHosts": "*",
  "Cors": {
    "Enabled": false,
    "AllowedOrigins": []
  },
  "Application": {
    "Name": "Expenso",
    "Version": "0.0.0"
  },
  "Authentication": {
    "AuthServer": "Keycloak"
  },
  "BudgetSharing": {
    "ExpirationDays": 3
  },
  "Keycloak": {
    "realm": "expenso",
    "auth-server-url": "http://localhost:9000/",
    "ssl-required": "none",
    "resource": "expenso_app",
    "verify-token-audience": false,
    "credentials": {
      "secret": ""
    }
  },
  "EfCore": {
    "ConnectionParameters": {
      "Host": "127.0.0.1",
      "Port": "5432",
      "DefaultDatabase": "postgres",
      "Database": "expenso",
      "User": "",
      "Password": ""
    },
    "InMemory": false,
    "UseMigration": true,
    "UseSeeding": true
  },
  "Files": {
    "StorageType": "Disk",
    "ImportDirectory": "Import",
    "ReportsDirectory": "Reports"
  },
  "Notifications": {
    "Enabled": true,
    "InApp": {
      "Enabled": true
    },
    "Email": {
      "Enabled": true,
      "ReplyTo": "",
      "From": "",
      "Smtp": {
        "Host": "127.0.0.1",
        "Port": 25,
        "Ssl": false,
        "Username": "",
        "Password": ""
      }
    },
    "Push": {
      "Enabled": true
    }
  },
  "Otlp": {
    "ServiceName": "Expenso",
    "Endpoint": "http://otel-collector:4317"
  }
}